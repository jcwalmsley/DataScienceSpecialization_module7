y <- mtcars$mpg
x <- (mtcars$cyl + mtcars$disp + mtcars$wt + as.numeric(mtcars$am))
n <- length(y)
fw <- lm(y ~ x, data = ordmtcars)
coef(summary(fw))
e <- resid(fw)# ;e;plot(e);sum(e)
plot(x,e,
xlab = "cyl + disp + wt + am",
ylab = "Residuals (MPG)",
bg = "lightblue",
col = "black", cex = 1.5, pch = 21, frame = FALSE)
abline(h = 0, lwd = 2)
for(i in 1:n)
lines(c(x[i], x[i]), c(e[i], 0), col = "red", lwd = 2)
