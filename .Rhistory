g <- ggplot(mtcars, aes(x = drat + hp + wt + am, y = mpg),)
g <- g + xlab(">drat + hp + wt + am<")
g <- g + ylab("Miles per Gallon")
g <- g + geom_point(size = 3.0, col = "blue", alpha = 0.55)
g <- g + geom_smooth(method = "lm", col = "black")
g
